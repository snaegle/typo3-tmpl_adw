<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<type>module</type>
		<description>
			Language labels for form labels.
			The file »locallang-form.xml« will be loaded from the path set as
			»settings.languageRootPath« in TypoScript.

			Localisation keys / label indexes follow the convention:
				* label for input FOO: input.FOO
				* label for child-item BAR in control FOO (e.g. select options): input.FOO-BAR
				* placeholder for input FOO: input.FOO.placeholder
		</description>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="button.empty">Suchfelder leeren</label>
			<label index="input.default">Alle Felder</label>
			<label index="input.raw">Solr Abfrage</label>
			<label index="input.klostername">Klostername</label>
			<label index="input.kloster-id">Klosternummer</label>
			<label index="input.bistum">Bistum</label>
			<label index="input.orden">Orden</label>
			<label index="input.ort">Ort</label>
			<label index="input.zeitraum">Zeitraum</label>
			<label index="input.zeitraum-from">Zeitraum von-</label>
			<label index="input.zeitraum-to">bis</label>
			<label index="input.zeitorden-fromto">Zeitraum Orden von-bis</label>
			<label index="input.zeitstandort-fromto">Zeitraum Standort von-bis</label>
			<label index="facet.resetView">Kartenansicht zurücksetzen</label>
			<label index="input.person">Personenname</label>
			<label index="input.person-checkbox">ähnliche Personennamen einschließen</label>
			<label index="input.status">Klosterstatus</label>
			<label index="noresults">Keine Treffer</label>
		</languageKey>
	</data>
</T3locallang>