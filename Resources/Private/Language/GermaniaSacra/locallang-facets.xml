<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<type>module</type>
		<description>
			Language labels for facet content.
			The file »locallang-facets.xml« will be loaded from the path set as
			»settings.languageRootPath« in TypoScript.

			Localisation keys / label indexes follow the convention:
				* facet name for index FOO: facet.FOO
				* facet name for value BAR in index FOO: facet.FOO.BAR
		</description>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="facet.map">Karte</label>
			<label index="facet.list">Liste</label>
			<label index="facet.view">Ansicht</label>
			<label index="facet.legend"> Legende</label>
			<label index="facet.resetView">Kartenansicht zurücksetzen</label>
			<label index="facet.orden">Orden</label>
			<label index="facet.jahr50">Zeitraum</label>
			<label index="facet.jahr50orden">Zeitraum Orden</label>
			<label index="facet.jahr50standort">Zeitraum Standort</label>
			<label index="facet.bistum">Bistum</label>
			<label index="facet.band">Germania Sacra Band</label>
			<label index="facet.band.hat_band">Alle Bände</label>
		</languageKey>
	</data>
</T3locallang>